  ## Genepop - filtering individuals w/low representation {R}

### read genepop file
library(adegenet)
library(ape)
library(hierfstat)
x <- read.genepop("populations.snps.gen")
x

### See proportion of SNPs in each individual
prop_typed_ind <- propTyped(x)
hist(prop_typed_ind, col = "grey", border = "white", nclass = 30,
     main = "prop typed in individuals", xlab = "prop loci typed")

prop_typed_ind

### Set a threshold for individuals filtering
IND_THRESHOLD <- 0.1

### Apply threshold
x <- x[prop_typed_ind > IND_THRESHOLD]
x

### Save the SNPs

loci<-locNames(x)
write.csv(loci, file="whitelist_2380SNPs.csv")

### Save the individuals

sam<-indNames(x)
write.csv(sam, file="indNames.csv")

